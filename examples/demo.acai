// examples/demo.acai
function soma(a, b) {
  let s = a + b;
  return s;
}

function main() {
  let x = 1;
  let y = 2;
  if (x < y) {
    x = soma(x, y);
  } else {
    x = 0;
  }

  let i = 0;
  while (i < 3) {
    i = i + 1;
  }

  do {
    x = x - 1;
  } while (x > 0);

  for (let j = 0; j < 2; j = j + 1) {
    x = x + j;
  }
}
